{
  "name": "@niewicz/mde",
  "description": "Angular Material Extended",
  "version": "14.0.0",
  "author": "Joe Jordan Brown <joe.jordan.brown@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/material-extended/mde.git"
  },
  "bugs": {
    "url": "https://github.com/material-extended/mde/issues"
  },
  "keywords": [
    "angular",
    "angular popover",
    "material",
    "material design",
    "material extended",
    "material popover",
    "components",
    "popover"
  ],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build:library && npm run serve",
    "serve": "ng serve --aot",
    "build": "ng build",
    "build:library": "ng build @material-extended/mde",
    "build:library:prod": "ng build @material-extended/mde --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "core-js": "^3.25.5",
    "rxjs": "^7.5.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@angular/language-service": "^14.2.5",
    "@types/jasmine": "^4.3.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.8.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.2.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  },
  "homepage": "https://github.com/material-extended/mde#readme",
  "main": "index.d.ts"
